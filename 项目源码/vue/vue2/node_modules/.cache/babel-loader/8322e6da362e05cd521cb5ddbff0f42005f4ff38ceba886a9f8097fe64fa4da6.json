ms.pageSize = pageSize;\n      this.getMyfans();\n    },\n    handleCurrentChange(currentPage) {\n      this.params.pageNum = currentPage;\n      this.getMyfans();\n    }\n  }\n};","map":{"version":3,"names":["request","name","data","user","localStorage","getItem","JSON","parse","params","idlist","pageNum","pageSize","total","allData","isfollow","Myfollow","FollowForm","userid","followid","userdata","fandata","created","getMyfans","getMyfollow","mounted","methods","get","id","then","res","code","getMyFansInfo","getUserData","$message","type","message","msg","post","list","personal","$router","push","path","follow","includes","handleResponse","myfollow","myfan","updateUser","handleError","error","console","catch","delete","handleSizeChange","handleCurrentChange","currentPage"],"sources":["src/views/person/MyFanView.vue"],"sourcesContent":["<template xmlns=\"http://www.w3.org/1999/html\">\r\n  <div class=\"fan_box\">\r\n      <div class=\"fan\" v-for=\"(item, index) in allData\">\r\n          <div class=\"fan_left\">\r\n            <img class=\"fan_img\" :src=\"'http://localhost:8080/api/files/' + item.avatar\" />\r\n          </div>\r\n          <div class=\"fan_info\">\r\n            <div class=\"fan_info_top\">\r\n              <span\r\n                  style=\"color: #666; max-width: 180px\"\r\n                  @click=\"personal(item.id)\"\r\n              >{{ item.username }}</span\r\n              >\r\n            </div>\r\n            <div class=\"fan_info_bottom\">\r\n              <span @click=\"personal(item.id)\">{{ item.specialty }}</span>\r\n            </div>\r\n          </div>\r\n          <div class=\"fan_botton\">\r\n            <el-button\r\n                @click=\"follow(item)\"\r\n                type=\"primary\"\r\n                size=\"small\"\r\n                round\r\n                icon=\"el-icon-check\"\r\n                v-text=\"Myfollow.indexOf(item.id) > -1 ? '已关注' : '关注'\"\r\n            ></el-button>\r\n          </div>\r\n      