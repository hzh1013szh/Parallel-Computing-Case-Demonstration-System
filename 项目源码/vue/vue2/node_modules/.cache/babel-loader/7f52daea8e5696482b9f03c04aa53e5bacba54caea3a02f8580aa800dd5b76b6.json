{"ast":null,"code":"import request from \"@/utils/request\";\nexport default {\n  name: \"AllPostView\",\n  data() {\n    return {\n      params: {\n        pageNum: 1,\n        pageSize: 5\n      },\n      total: '',\n      post: {\n        title: '',\n        likes: '',\n        comments: '',\n        img: '',\n        favorites: '',\n        description: '',\n        userid: '',\n        //用户的id\n        time: ''\n      },\n      username: '',\n      //用户的用户名,\n      avatar: ''\n    };\n  },\n  created() {\n    this.loadpost();\n  },\n  mounted() {\n    this.loadpost();\n  },\n  methods: {\n    loadpost() {\n      request.get(`/post`).then(res => {\n        if (res.code === \"0\") {\n          this.post = res.data[0];\n          console.log(res.data);\n          this.loaduser();\n        } else {\n          this.$message({\n            type: 'error',\n            message: res.msg\n          });\n        }\n      });\n    },\n    loaduser() {\n      request.get(`/admin/search/${this.post.userid}`).then(res => {\n        if (res.code === \"0\") {\n          console.log(res.data);\n          this.username = res.data.userid;\n          this.avatar = res.data.avatar;\n        } else {\n          this.$message({\n            type: 'error',\n            message: res.msg\n          });\n        }\n      });\n    },\n    handleSizeChange() {},\n    handleCurrentChange() {}\n  }\n};","map":{"version":3,"names":["request","name","data","params","pageNum","pageSize","total","post","title","likes","comments","img","favorites","description","userid","time","username","avatar","created","loadpost","mounted","methods","get","then","res","code","console","log","loaduser","$message","type","message","msg","handleSizeChange","handleCurrentChange"],"sources":["src/views/AllPostView.vue"],"sourcesContent":["\r\n<template>\r\n  <div style=\"margin-top: 15px\">\r\n    <el-card class=\"me-area\"\r\n             :body-style=\"{ padding: '16px' }\">\r\n      <div class=\"me-article-header\">\r\n        <a @click=\"view(id)\"\r\n           class=\"me-article-title\">{{post.title}}</a>\r\n        <span class=\"me-pull-right me-article-count\">\r\n            <i class=\"el-icon-star-off\"></i>&nbsp;{{post.favorites}}\r\n          </span>\r\n        <span class=\"me-pull-right me-article-count\">\r\n            <i class=\"el-icon-chat-dot-round\"></i>&nbsp;{{post.comments}}\r\n          </span>\r\n        <span class=\"me-pull-right me-article-count\">\r\n            <img style=\"height:13px;width:13px\" src=\"@/assets/good.png\"></img>&nbsp;{{post.likes}}\r\n          </span>\r\n\r\n      </div>\r\n\r\n      <div class=\"me-artile-description\">\r\n        {{post.description}}\r\n      </div>\r\n      <div class=\"preimg\" v-if=\"post.img\">\r\n        <el-image\r\n            style=\"width: 128px; height: 84px\"\r\n            :src=\"'http://localhost:8080/api/files/' + post.img\"\r\n            :preview-src-list=\"['http://localhost:8080/api/files/' + post.img]\">\r\n        </el-image>\r\n      </div>\r\n      <div class=\"me-article-footer\">\r\n          <span class=\"me-article-author\" @click=\"personal(id)\">\r\n            <i class=\"me-icon-author\"></i>&nbsp;{{username}}\r\n            <el-avatar size=\"50\" :src=\"'http://localhost:8080/api/files/' + avatar\"></el-avatar>\r\n          </span>\r\n\r\n        <div class=\"me-pull-right me-article-time\">\r\n          <span class=\"me-pull-right me-article-count\">\r\n            <i class=\"el-icon-time\"></i>&nbsp;{{post.time}}\r\n          </span>\r\n        </div>\r\n      </div>\r\n    </el-card>\r\n<!--    <el-pagination-->\r\n<!--        @size-change=\"handleSizeChange\"-->\r\n<!--        @current-change=\"handleCurrentChange\"-->\r\n<!--        :current-page=\"params.pageNum\"-->\r\n<!--        :page-sizes=\"[5, 10, 15, 20]\"-->\r\n<!--        :page-size=\"params.pageSize\"-->\r\n<!--        layout=\"total, sizes, prev, pager, next, jumper\"-->\r\n<!--        :total=\"total\">-->\r\n<!--    </el-pagination>-->\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport request from \"@/utils/request\";\r\n\r\nexport default {\r\n  name: \"AllPostView\",\r\n  data(){\r\n    return{\r\n      params:{\r\n        pageNum: 1,\r\n        pageSize: 5,\r\n      },\r\n      total: '',\r\n      post:{\r\n        title: '',\r\n        likes:'',\r\n        comments:'',\r\n        img:'',\r\n        favorites:'',\r\n        description:'',\r\n        userid:'',//用户的id\r\n        time:'',\r\n      },\r\n      username:'',//用户的用户名,\r\n      avatar:'',\r\n    }\r\n  },\r\n  created() {\r\n    this.loadpost();\r\n  },\r\n  mounted() {\r\n    this.loadpost();\r\n  },\r\n  methods: {\r\n    loadpost(){\r\n      request.get(`/post`).then(res => {\r\n        if(res.code === \"0\"){\r\n          this.post = res.data[0];\r\n          console.log(res.data);\r\n          this.loaduser();\r\n        }\r\n        else{\r\n          this.$message({\r\n            type: 'error',\r\n            message: res.msg\r\n          });\r\n        }\r\n      })\r\n    },\r\n    loaduser(){\r\n      request.get(`/admin/search/${this.post.userid}`).then(res => {\r\n        if(res.code === \"0\"){\r\n          console.log(res.data)\r\n          this.username = res.data.userid;\r\n          this.avatar = res.data.avatar;\r\n        }\r\n        else{\r\n          this.$message({\r\n            type: 'error',\r\n            message: res.msg\r\n          });\r\n        }\r\n      })\r\n    },\r\n    handleSizeChange(){\r\n\r\n    },\r\n    handleCurrentChange(){\r\n\r\n    }\r\n\r\n  }\r\n}\r\n</script>\r\n<style scoped>\r\n.me-article-header {\r\n  /*padding: 10px 18px;*/\r\n  padding-bottom: 10px;\r\n}\r\n\r\n.me-article-title {\r\n  font-weight: 600;\r\n}\r\n\r\n.me-article-icon {\r\n  padding: 3px 8px;\r\n}\r\n\r\n.me-article-count {\r\n  color: #a6a6a6;\r\n  padding-left: 20px;\r\n  font-size: 13px;\r\n}\r\n\r\n.me-pull-right {\r\n  float: right;\r\n}\r\n\r\n.me-artile-description {\r\n  font-size: 13px;\r\n  line-height: 24px;\r\n  margin-bottom: 10px;\r\n}\r\n.me-article-author {\r\n  color: #a6a6a6;\r\n  padding-right: 18px;\r\n  font-size: 13px;\r\n  cursor: pointer;\r\n}\r\n.me-article-author-img {\r\n  width: 15px;\r\n  height: 15px;\r\n}\r\n.me-article-author-avatar {\r\n  width: 30px;\r\n  height: 30px;\r\n  vertical-align: text-bottom;\r\n  border-radius: 50%;\r\n}\r\n.el-tag {\r\n  margin-left: 6px;\r\n}\r\n.me-article-time {\r\n  margin-top: 20px;\r\n}\r\n.leftimg {\r\n  /* width:11%;\r\n  height:11%; */\r\n  width: 100px;\r\n  height: 90px;\r\n  float: left;\r\n  margin-right: 20px;\r\n}\r\nimg {\r\n  width: 100%;\r\n  height: 100%;\r\n}\r\n.preimg {\r\n  width: 700px;\r\n  height: 90px;\r\n  margin-bottom: 10px;\r\n  display: flex;\r\n}\r\n</style>\r\n"],"mappings":"AAyDA,OAAAA,OAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,MAAA;QACAC,OAAA;QACAC,QAAA;MACA;MACAC,KAAA;MACAC,IAAA;QACAC,KAAA;QACAC,KAAA;QACAC,QAAA;QACAC,GAAA;QACAC,SAAA;QACAC,WAAA;QACAC,MAAA;QAAA;QACAC,IAAA;MACA;MACAC,QAAA;MAAA;MACAC,MAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,QAAA;EACA;EACAC,QAAA;IACA,KAAAD,QAAA;EACA;EACAE,OAAA;IACAF,SAAA;MACAnB,OAAA,CAAAsB,GAAA,UAAAC,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAlB,IAAA,GAAAiB,GAAA,CAAAtB,IAAA;UACAwB,OAAA,CAAAC,GAAA,CAAAH,GAAA,CAAAtB,IAAA;UACA,KAAA0B,QAAA;QACA,OACA;UACA,KAAAC,QAAA;YACAC,IAAA;YACAC,OAAA,EAAAP,GAAA,CAAAQ;UACA;QACA;MACA;IACA;IACAJ,SAAA;MACA5B,OAAA,CAAAsB,GAAA,uBAAAf,IAAA,CAAAO,MAAA,IAAAS,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACAC,OAAA,CAAAC,GAAA,CAAAH,GAAA,CAAAtB,IAAA;UACA,KAAAc,QAAA,GAAAQ,GAAA,CAAAtB,IAAA,CAAAY,MAAA;UACA,KAAAG,MAAA,GAAAO,GAAA,CAAAtB,IAAA,CAAAe,MAAA;QACA,OACA;UACA,KAAAY,QAAA;YACAC,IAAA;YACAC,OAAA,EAAAP,GAAA,CAAAQ;UACA;QACA;MACA;IACA;IACAC,iBAAA,GAEA;IACAC,oBAAA,GAEA;EAEA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}