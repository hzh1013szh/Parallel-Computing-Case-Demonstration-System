{"ast":null,"code":"import request from \"@/utils/request\";\nexport default {\n  name: \"MyFanView\",\n  data() {\n    return {\n      user: localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")) : {},\n      params: {\n        idlist: [],\n        pageNum: 1,\n        pageSize: 3\n      },\n      total: 0,\n      allData: [],\n      Myfollow: []\n    };\n  },\n  created() {\n    this.getMyfans();\n  },\n  mounted() {\n    this.getMyfans();\n  },\n  methods: {\n    getMyfans() {\n      request.get(`/follow/myfan/${this.user.id}`).then(res => {\n        if (res.code === \"0\") {\n          this.params.idlist = res.data;\n          console.log(this.params.idlist);\n          this.getMyFansInfo();\n        } else {\n          this.$message({\n            type: 'error',\n            message: res.msg\n          });\n        }\n      });\n    },\n    getMyFansInfo() {\n      request.post(\"/admin/search/idlist\", this.params).then(res => {\n        if (res.code === \"0\") {\n          this.allData = res.data.list;\n          this.total = res.data.total;\n        } else {\n          this.$message({\n            type: 'error',\n            message: res.msg\n          });\n        }\n      });\n    },\n    handleSizeChange(pageSize) {\n      this.params.pageSize = pageSize;\n      this.getMyfans();\n    },\n    handleCurrentChange(currentPage) {\n      this.params.pageNum = currentPage;\n      this.getMyfans();\n    }\n  }\n};","map":{"version":3,"names":["request","name","data","user","localStorage","getItem","JSON","parse","params","idlist","pageNum","pageSize","total","allData","Myfollow","created","getMyfans","mounted","methods","get","id","then","res","code","console","log","getMyFansInfo","$message","type","message","msg","post","list","handleSizeChange","handleCurrentChange","currentPage"],"sources":["src/views/person/MyFanView.vue"],"sourcesContent":["<template xmlns=\"http://www.w3.org/1999/html\">\r\n  <div class=\"fan_box\">\r\n      <div class=\"fan\" v-for=\"(item, index) in allData\">\r\n          <div class=\"fan_left\">\r\n            <img class=\"fan_img\" v-image-preview :src=\"'http://localhost:8080/api/files/' + item.avatar\" />\r\n          </div>\r\n          <div class=\"fan_info\">\r\n            <div class=\"fan_info_top\">\r\n              <span\r\n                  style=\"color: #666; max-width: 180px\"\r\n                  @click=\"personal(item.id)\"\r\n              >{{ item.username }}</span\r\n              >\r\n            </div>\r\n            <div class=\"fan_info_bottom\">\r\n              <span @click=\"personal(item.id)\">{{ item.specialty }}</span>\r\n            </div>\r\n          </div>\r\n          <div class=\"fan_botton\">\r\n            <el-button\r\n                @click=\"follow(item.id)\"\r\n                type=\"primary\"\r\n                size=\"small\"\r\n                round\r\n                icon=\"el-icon-check\"\r\n                v-text=\"Myfollow.indexOf(item.id) > -1 ? '已关注' : '关注'\"\r\n            ></el-button>\r\n          </div>\r\n        </div>\r\n      <div>\r\n        <el-empty\r\n            v-if=\"allData.length == 0\"\r\n            :image-size=\"250\"\r\n            description=\"这里什么都没有哟\"\r\n        ></el-empty>\r\n      </div>\r\n    <div style=\"margin-top: 15px\">\r\n      <el-pagination\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"params.pageNum\"\r\n          :page-sizes=\"[3, 6, 9, 12]\"\r\n          :page-size=\"params.pageSize\"\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :total=\"total\"\r\n      >\r\n      </el-pagination>\r\n    </div>>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\n\r\nexport default {\r\n  name: \"MyFanView\",\r\n  data(){\r\n    return{\r\n      user: localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")) : {},\r\n      params:{\r\n        idlist:[],\r\n        pageNum: 1,\r\n        pageSize: 3,\r\n      },\r\n      total: 0,\r\n      allData: [],\r\n      Myfollow:[],\r\n    }\r\n  },\r\n  created() {\r\n    this.getMyfans();\r\n  },\r\n  mounted() {\r\n    this.getMyfans();\r\n  },\r\n  methods: {\r\n    getMyfans(){\r\n      request.get(`/follow/myfan/${this.user.id}`).then(res => {\r\n        if(res.code === \"0\"){\r\n          this.params.idlist = res.data;\r\n          console.log(this.params.idlist);\r\n          this.getMyFansInfo();\r\n        }\r\n        else{\r\n          this.$message({\r\n            type: 'error',\r\n            message: res.msg\r\n          });\r\n        }\r\n      })\r\n    },\r\n    getMyFansInfo(){\r\n      request.post(\"/admin/search/idlist\",this.params).then(res =>{\r\n        if(res.code === \"0\"){\r\n          this.allData = res.data.list;\r\n          this.total = res.data.total;\r\n        }\r\n        else{\r\n          this.$message({\r\n            type: 'error',\r\n            message: res.msg\r\n          });\r\n        }\r\n      })\r\n    },\r\n    handleSizeChange(pageSize){\r\n      this.params.pageSize = pageSize;\r\n      this.getMyfans();\r\n    },\r\n    handleCurrentChange(currentPage){\r\n      this.params.pageNum = currentPage;\r\n      this.getMyfans();\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n.fan_box :hover {\r\n  border-width: 1px;\r\n  border-color: deepskyblue;\r\n}\r\n.fan {\r\n  padding: 15px 40px 15px 30px;\r\n  height: 50px;\r\n  display: flex;\r\n  align-items: center;\r\n  border: 1px solid #ebebeb;\r\n}\r\n.fan :hover {\r\n  border-width: 1px;\r\n  border-color: deepskyblue;\r\n}\r\n.fan_left {\r\n  width: 60px;\r\n  height: 60px;\r\n}\r\n.fan_img {\r\n  object-fit: cover;\r\n  margin-top: 1px;\r\n  width: 50px;\r\n  height: 50px;\r\n  border-radius: 50%;\r\n  border: 1px solid #ebebeb;\r\n  vertical-align: top;\r\n}\r\n.fan_info {\r\n  display: inline-block;\r\n  margin-left: 20px;\r\n  -webkit-box-flex: 1;\r\n  -ms-flex-positive: 1;\r\n  flex-grow: 1;\r\n  overflow: hidden;\r\n}\r\n.fan_info_top {\r\n  display: inline-block;\r\n  font-size: 10;\r\n  line-height: 14px;\r\n  vertical-align: top;\r\n  cursor: pointer;\r\n}\r\n.fan_info_top :hover {\r\n  color: deepskyblue;\r\n}\r\n.fan_info_bottom {\r\n  line-height: 14px;\r\n  color: #999;\r\n  margin-top: 5px;\r\n  cursor: pointer;\r\n}\r\n.fan_info_bottom :hover {\r\n  color: deepskyblue;\r\n}\r\n</style>"],"mappings":"AAoDA,OAAAA,OAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,IAAA,EAAAC,YAAA,CAAAC,OAAA,WAAAC,IAAA,CAAAC,KAAA,CAAAH,YAAA,CAAAC,OAAA;MACAG,MAAA;QACAC,MAAA;QACAC,OAAA;QACAC,QAAA;MACA;MACAC,KAAA;MACAC,OAAA;MACAC,QAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,SAAA;EACA;EACAC,QAAA;IACA,KAAAD,SAAA;EACA;EACAE,OAAA;IACAF,UAAA;MACAhB,OAAA,CAAAmB,GAAA,uBAAAhB,IAAA,CAAAiB,EAAA,IAAAC,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAf,MAAA,CAAAC,MAAA,GAAAa,GAAA,CAAApB,IAAA;UACAsB,OAAA,CAAAC,GAAA,MAAAjB,MAAA,CAAAC,MAAA;UACA,KAAAiB,aAAA;QACA,OACA;UACA,KAAAC,QAAA;YACAC,IAAA;YACAC,OAAA,EAAAP,GAAA,CAAAQ;UACA;QACA;MACA;IACA;IACAJ,cAAA;MACA1B,OAAA,CAAA+B,IAAA,8BAAAvB,MAAA,EAAAa,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAV,OAAA,GAAAS,GAAA,CAAApB,IAAA,CAAA8B,IAAA;UACA,KAAApB,KAAA,GAAAU,GAAA,CAAApB,IAAA,CAAAU,KAAA;QACA,OACA;UACA,KAAAe,QAAA;YACAC,IAAA;YACAC,OAAA,EAAAP,GAAA,CAAAQ;UACA;QACA;MACA;IACA;IACAG,iBAAAtB,QAAA;MACA,KAAAH,MAAA,CAAAG,QAAA,GAAAA,QAAA;MACA,KAAAK,SAAA;IACA;IACAkB,oBAAAC,WAAA;MACA,KAAA3B,MAAA,CAAAE,OAAA,GAAAyB,WAAA;MACA,KAAAnB,SAAA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}