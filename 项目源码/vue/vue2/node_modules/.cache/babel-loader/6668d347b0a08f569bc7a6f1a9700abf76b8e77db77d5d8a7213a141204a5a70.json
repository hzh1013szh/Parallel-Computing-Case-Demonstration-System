{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport request from \"@/utils/request\";\nexport default {\n  name: \"PostItem\",\n  props: {\n    id: Number,\n    title: String,\n    favorites: Number,\n    comments: Number,\n    goods: Number,\n    description: String,\n    img: String,\n    userid: Number,\n    time: String,\n    postCategoryName: String\n  },\n  data() {\n    return {\n      username: '',\n      //用户的用户名,\n      avatar: ''\n    };\n  },\n  created() {\n    this.loaduser();\n  },\n  mounted() {\n    this.loaduser();\n  },\n  methods: {\n    loaduser() {\n      request.get(`/admin/search/${this.userid}`).then(res => {\n        if (res.code === \"0\") {\n          this.username = res.data.username;\n          this.avatar = res.data.avatar;\n        } else {\n          this.$message({\n            type: 'error',\n            message: res.msg\n          });\n        }\n      });\n    },\n    personal(id) {\n      this.$router.push(`/user/${id}`);\n    },\n    view(id) {\n      this.$router.push(`/view/${id}`);\n    }\n  }\n};","map":{"version":3,"names":["request","name","props","id","Number","title","String","favorites","comments","goods","description","img","userid","time","postCategoryName","data","username","avatar","created","loaduser","mounted","methods","get","then","res","code","$message","type","message","msg","personal","$router","push","view"],"sources":["src/views/Post/PostItem.vue"],"sourcesContent":["\r\n\r\n<template>\r\n\r\n  <div style=\"margin-top: 15px\">\r\n    <el-card class=\"me-area\"\r\n             :body-style=\"{ padding: '16px' }\">\r\n      <div class=\"me-article-header\">\r\n        <a @click=\"view(id)\"\r\n           class=\"me-article-title\">{{title}}</a>\r\n        <span class=\"me-pull-right me-article-count\">\r\n            <i class=\"el-icon-star-off\"></i>&nbsp;{{favorites}}\r\n          </span>\r\n        <span class=\"me-pull-right me-article-count\">\r\n            <i class=\"el-icon-chat-dot-round\"></i>&nbsp;{{comments}}\r\n          </span>\r\n        <span class=\"me-pull-right me-article-count\">\r\n            <img style=\"height:13px;width:13px\" src=\"../../assets/good.png\"></img>&nbsp;{{goods}}\r\n          </span>\r\n\r\n      </div>\r\n\r\n      <div class=\"me-artile-description\">\r\n        {{description}}\r\n      </div>\r\n      <div class=\"preimg\" v-if=\"img\">\r\n        <el-image\r\n            style=\"object-fit: cover\"\r\n            :src=\"'http://localhost:8080/api/files/' + img\"\r\n            :preview-src-list=\"['http://localhost:8080/api/files/' + img]\">\r\n        </el-image>\r\n      </div>\r\n      <div class=\"me-article-footer\">\r\n          <span class=\"me-article-author\" @click=\"personal(userid)\">\r\n            <i class=\"me-icon-author\"></i>&nbsp;{{username}}\r\n            <el-image :src=\"'http://localhost:8080/api/files/' + avatar\" class=\"me-article-author-avatar\"></el-image>\r\n          </span>\r\n        <div class=\"me-postCategory\">\r\n          分类：{{postCategoryName}}\r\n        </div>\r\n        <div class=\"me-pull-right me-article-time\">\r\n          <span class=\"me-pull-right me-article-count\">\r\n            <i class=\"el-icon-time\"></i>&nbsp;{{time}}\r\n          </span>\r\n        </div>\r\n      </div>\r\n    </el-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\n\r\nexport default {\r\n  name: \"PostItem\",\r\n  props:{\r\n    id: Number,\r\n    title: String,\r\n    favorites: Number,\r\n    comments: Number,\r\n    goods: Number,\r\n    description: String,\r\n    img: String,\r\n    userid: Number,\r\n    time: String,\r\n    postCategoryName: String,\r\n  },\r\n  data() {\r\n    return {\r\n      username: '',//用户的用户名,\r\n      avatar:''\r\n    }\r\n  },\r\n  created() {\r\n    this.loaduser();\r\n  },\r\n  mounted() {\r\n    this.loaduser();\r\n  },\r\n  methods:{\r\n    loaduser(){\r\n      request.get(`/admin/search/${this.userid}`).then(res => {\r\n        if(res.code === \"0\"){\r\n          this.username = res.data.username;\r\n          this.avatar = res.data.avatar;\r\n        }\r\n        else{\r\n          this.$message({\r\n            type: 'error',\r\n            message: res.msg\r\n          });\r\n        }\r\n      })\r\n    },\r\n    personal(id){\r\n      this.$router.push(`/user/${id}`)\r\n    },\r\n    view(id){\r\n      this.$router.push(`/view/${id}`)\r\n    },\r\n  }\r\n}\r\n</script>\r\n<style scoped>\r\n.me-article-header {\r\n  /*padding: 10px 18px;*/\r\n  padding-bottom: 10px;\r\n}\r\n\r\n.me-article-title {\r\n  font-weight: 600;\r\n}\r\n\r\n.me-article-icon {\r\n  padding: 3px 8px;\r\n}\r\n\r\n.me-article-count {\r\n  color: #a6a6a6;\r\n  padding-left: 20px;\r\n  font-size: 13px;\r\n}\r\n\r\n.me-pull-right {\r\n  float: right;\r\n}\r\n\r\n.me-artile-description {\r\n  font-size: 13px;\r\n  line-height: 24px;\r\n  margin-bottom: 10px;\r\n}\r\n.me-article-author {\r\n  color: #a6a6a6;\r\n  padding-right: 18px;\r\n  font-size: 13px;\r\n  cursor: pointer;\r\n}\r\n.me-postCategory{\r\n  font-size: 15px;\r\n  color: #0271ff;\r\n}\r\n.me-article-author-img {\r\n  width: 15px;\r\n  height: 15px;\r\n}\r\n.me-article-author-avatar {\r\n  object-fit: cover;\r\n  width: 30px;\r\n  height: 30px;\r\n  vertical-align: text-bottom;\r\n  border-radius: 50%;\r\n}\r\n.el-tag {\r\n  margin-left: 6px;\r\n}\r\n.me-article-time {\r\n  margin-top: 20px;\r\n}\r\n.leftimg {\r\n  /* width:11%;\r\n  height:11%; */\r\n  width: 100px;\r\n  height: 90px;\r\n  float: left;\r\n  margin-right: 20px;\r\n}\r\nimg {\r\n  width: 100%;\r\n  height: 100%;\r\n}\r\n.preimg {\r\n  width: 700px;\r\n  height: 90px;\r\n  margin-bottom: 10px;\r\n  display: flex;\r\n}\r\n</style>\r\n"],"mappings":";AAmDA,OAAAA,OAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACAC,EAAA,EAAAC,MAAA;IACAC,KAAA,EAAAC,MAAA;IACAC,SAAA,EAAAH,MAAA;IACAI,QAAA,EAAAJ,MAAA;IACAK,KAAA,EAAAL,MAAA;IACAM,WAAA,EAAAJ,MAAA;IACAK,GAAA,EAAAL,MAAA;IACAM,MAAA,EAAAR