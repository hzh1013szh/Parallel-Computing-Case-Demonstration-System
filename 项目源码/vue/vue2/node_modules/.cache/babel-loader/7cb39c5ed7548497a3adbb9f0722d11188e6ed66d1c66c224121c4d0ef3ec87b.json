{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport request from \"@/utils/request\";\nexport default {\n  name: \"CommentItem\",\n  props: {\n    comment: Object,\n    postid: Number\n  },\n  data() {\n    return {\n      reply: this.getEmptyReply(),\n      commentShow: false,\n      commentShowIndex: '',\n      placeholder: '你的评论...',\n      user: localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")) : {}\n    };\n  },\n  methods: {\n    showComment(commentShowIndex, toUser) {\n      this.reply = this.getEmptyReply();\n      if (this.commentShowIndex !== commentShowIndex) {\n        if (toUser) {\n          this.placeholder = `@${toUser.username} `;\n          this.reply.toUserId = toUser.id;\n        } else {\n          this.placeholder = '你的评论...';\n        }\n        this.commentShow = true;\n        this.commentShowIndex = commentShowIndex;\n      } else {\n        this.commentShow = false;\n        this.commentShowIndex = '';\n      }\n    },\n    getEmptyReply() {\n      return {\n        content: '',\n        toUserid: '',\n        RootCommentid: this.comment.id,\n        postid: this.postid\n      };\n    },\n    publishComment() {\n      if (!this.reply.content) {\n        return;\n      }\n      this.reply.userid = this.user.id;\n      this.reply.postid = this.postid;\n      request.post(\"/comment\", this.reply).then(res => {\n        if (res.code === \"0\") {\n          this.$message({\n            type: 'success',\n            message: '评论成功'\n          });\n          this.showComment(this.commentShowIndex);\n          this.$emit(\"IncreasePostComment\");\n          this.$emit(\"getPost\");\n        } else {\n          this.$message({\n            type: 'error',\n            message: res.msg\n          });\n        }\n      });\n    },\n    toUser(id) {\n      this.$router.push(`/user/${id}`);\n    }\n  }\n};","map":{"version":3,"names":["request","name","props","comment","Object","postid","Number","data","reply","getEmptyReply","commentShow","commentShowIndex","placeholder","user","localStorage","getItem","JSON","parse","methods","showComment","toUser","username","toUserId","id","content","toUserid","RootCommentid","publishComment","userid","post","then","res","code","$message","type","message","$emit","msg","$router","push"],"sources":["src/views/Comment/CommentItem.vue"],"sourcesContent":["\r\n<template>\r\n  <div class=\"me-view-comment-item\">\r\n    <div class=\"me-view-comment-author\">\r\n      <a class=\"\" @click=\"toUser(comment.admin.id)\">\r\n        <img class=\"me-view-picture\" :src=\"'http://localhost:8080/api/files/' + comment.admin.avatar\"></img>\r\n      </a>\r\n      <div class=\"me-view-info\">\r\n        <span class=\"me-view-nickname\">{{comment.admin.username}}</span>\r\n        <div class=\"me-view-meta\">\r\n          <span>{{comment.time}}</span>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <div>\r\n      <p class=\"me-view-comment-content\">{{comment.content}}</p>\r\n      <div class=\"me-view-comment-tools\">\r\n        <!--<a class=\"me-view-comment-tool\">-->\r\n        <!--<i class=\"el-icon-caret-top\"></i> 20-->\r\n        <!--</a>-->\r\n        <a class=\"me-view-comment-tool\" @click=\"showComment(-1,comment.admin)\">\r\n          <i class=\"me-icon-comment\"></i>&nbsp; 评论\r\n        </a>\r\n      </div>\r\n      <div class=\"me-reply-list\">\r\n        <div class=\"me-reply-item\" v-for=\"c in comment.childrens\" :key=\"c.id\">\r\n          <div style=\"font-size: 14px\">\r\n            <span class=\"me-reply-user\">{{c.admin.username}}:&nbsp;&nbsp;</span>\r\n\r\n            <span v-if=\"c.touserid\" class=\"me-reply-user\">@{{c.toUser.username}} </span>\r\n\r\n            <span>{{c.content}}</span>\r\n          </div>\r\n          <div class=\"me-view-meta\">\r\n            <span style=\"padding-right: 10px\">{{c.time}}</span>\r\n            <!--  <a class=\"me-view-comment-tool\" @click=\"showComment(c.id, c.author)\">\r\n                <i class=\"me-icon-comment\"></i>&nbsp;回复\r\n              </a> -->\r\n          </div>\r\n\r\n        </div>\r\n\r\n        <div class=\"me-view-comment-write\" v-show=\"commentShow\">\r\n\r\n          <el-input\r\n              v-model=\"reply.content\"\r\n              type=\"input\"\r\n              style=\"width: 90%\"\r\n              :placeholder=\"placeholder\"\r\n              class=\"me-view-comment-text\"\r\n              resize=\"none\">\r\n          </el-input>\r\n\r\n          <el-button style=\"margin-left: 8px\" @click=\"publishComment()\" type=\"text\">评论</el-button>\r\n\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport request from \"@/utils/request\";\r\n\r\nexport default {\r\n  name: \"CommentItem\",\r\n  props:{\r\n    comment: Object,\r\n    postid: Number,\r\n  },\r\n  data(){\r\n    return{\r\n      reply: this.getEmptyReply(),\r\n      commentShow: false,\r\n      commentShowIndex: '',\r\n      placeholder: '你的评论...',\r\n      user: localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")) : {},\r\n    }\r\n  },\r\n  methods: {\r\n    showComment(commentShowIndex, toUser){\r\n      this.reply = this.getEmptyReply()\r\n\r\n      if (this.commentShowIndex !== commentShowIndex) {\r\n        if (toUser) {\r\n          this.placeholder = `@${toUser.username} `\r\n          this.reply.toUserId = toUser.id\r\n        } else {\r\n          this.placeholder = '你的评论...'\r\n        }\r\n\r\n        this.commentShow = true\r\n        this.commentShowIndex = commentShowIndex\r\n      } else {\r\n        this.commentShow = false\r\n        this.commentShowIndex = ''\r\n      }\r\n    },\r\n    getEmptyReply(){\r\n      return {\r\n        content: '',\r\n        toUserid: '',\r\n        RootCommentid: this.comment.id,\r\n        postid: this.postid,\r\n      }\r\n    },\r\n    publishComment(){\r\n      if(!this.reply.content){\r\n        return;\r\n      }\r\n      this.reply.userid = this.user.id;\r\n      this.reply.postid = this.postid;\r\n      request.post(\"/comment\",this.reply).then(res => {\r\n        if(res.code === \"0\"){\r\n          this.$message({\r\n            type: 'success',\r\n            message: '评论成功'\r\n          });\r\n          this.showComment(this.commentShowIndex);\r\n          this.$emit(\"IncreasePostComment\")\r\n          this.$emit(\"getPost\");\r\n        }\r\n        else{\r\n          this.$message({\r\n            type: 'error',\r\n            message: res.msg\r\n          });\r\n        }\r\n      })\r\n    },\r\n    toUser(id){\r\n      this.$router.push(`/user/${id}`);\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n.me-view-meta {\r\n\r\n  font-size: 14px;\r\n  color: #969696;\r\n}\r\n.me-view-picture {\r\n  width: 50px;\r\n  height: 50px;\r\n  border: 1px solid #ddd;\r\n  border-radius: 50%;\r\n  vertical-align: top;\r\n  background-color: #5fb878;\r\n}\r\n.me-view-tag-item {\r\n  margin: 0 4px;\r\n}\r\n\r\n.me-view-comment {\r\n  margin-top: 60px;\r\n}\r\n\r\n.me-view-comment-title {\r\n  font-weight: 600;\r\n  border-bottom: 1px solid #f0f0f0;\r\n  padding-bottom: 20px;\r\n}\r\n\r\n.me-view-comment-write {\r\n  margin-top: 20px;\r\n}\r\n\r\n.me-view-comment-text {\r\n  font-size: 16px;\r\n}\r\n\r\n.v-show-content {\r\n  padding: 8px 25px 15px 30px !important;\r\n}\r\n\r\n.v-note-wrapper .v-note-panel {\r\n  box-shadow: none !important;\r\n}\r\n\r\n.me-view-comment-item {\r\n  margin-top: 20px;\r\n  padding-bottom: 16px;\r\n  border-bottom: 1px solid #f0f0f0;\r\n}\r\n\r\n.me-view-comment-author {\r\n  margin: 10px 0;\r\n  vertical-align: middle;\r\n}\r\n\r\n.me-view-nickname {\r\n  font-size: 14px;\r\n}\r\n\r\n.me-view-comment-content {\r\n  line-height: 1.5;\r\n}\r\n\r\n.me-view-comment-tools {\r\n  margin-top: 4px;\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.me-view-comment-tool {\r\n  font-size: 13px;\r\n  color: #a6a6a6;\r\n  padding-right: 14px;\r\n}\r\n\r\n.v-note-wrapper .v-note-panel .v-note-show .v-show-content, .v-note-wrapper .v-note-panel .v-note-show .v-show-content-html {\r\n  background: #fff !important;\r\n}\r\n\r\n.me-reply-list {\r\n  padding-left: 16px;\r\n  border-left: 4px solid #c5cac3;\r\n}\r\n\r\n.me-reply-item {\r\n  margin-bottom: 8px;\r\n  border-bottom: 1px solid #f0f0f0;\r\n}\r\n\r\n.me-reply-user {\r\n  color: #78b6f7;\r\n}\r\n</style>\r\n"],"mappings":";AA6DA,OAAAA,OAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACAC,OAAA,EAAAC,MAAA;IACAC,MAAA,EAAAC;EACA;EACAC,KAAA;IACA;MACAC,KAAA,OAAAC,aAAA;MACAC,WAAA;MACAC,gBAAA;MACAC,WAAA;MACAC,IAAA,EAAAC,YAAA,CAAAC,OAAA,WAAAC,IAAA,CAAAC,KAAA,CAAAH,YAAA,CAAAC,OAAA;IACA;EACA;EACAG,OAAA;IACAC,YAAAR,gBAAA,EAAAS,MAAA;MACA,KAAAZ,KAAA,QAAAC,aAAA;MAEA,SAAAE,gBAAA,KAAAA,gBAAA;QACA,IAAAS,MAAA;UACA,KAAAR,WAAA,OAAAQ,MAAA,CAAAC,QAAA;UACA,KAAAb,KAAA,CAAAc,QAAA,GAAAF,MAAA,CAAAG,EAAA;QACA;UACA,KAAAX,WAAA;QACA;QAEA,KAAAF,WAAA;QACA,KAAAC,gBAAA,GAAAA,gBAAA;MACA;QACA,KAAAD,WAAA;QACA,KAAAC,gBAAA;MACA;IACA;IACAF,cAAA;MACA;QACAe,OAAA;QACAC,QAAA;QACAC,aAAA,OAAAvB,OAAA,CAAAoB,EAAA;QACAlB,MAAA,OAAAA;MACA;IACA;IACAsB,eAAA;MACA,UAAAnB,KAAA,CAAAgB,OAAA;QACA;MACA;MACA,KAAAhB,KAAA,CAAAoB,MAAA,QAAAf,IAAA,CAAAU,EAAA;MACA,KAAAf,KAAA,CAAAH,MAAA,QAAAA,MAAA;MACAL,OAAA,CAAA6B,IAAA,kBAAArB,KAAA,EAAAsB,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAC,QAAA;YACAC,IAAA;YACAC,OAAA;UACA;UACA,KAAAhB,WAAA,MAAAR,gBAAA;UACA,KAAAyB,KAAA;UACA,KAAAA,KAAA;QACA,OACA;UACA,KAAAH,QAAA;YACAC,IAAA;YACAC,OAAA,EAAAJ,GAAA,CAAAM;UACA;QACA;MACA;IACA;IACAjB,OAAAG,EAAA;MACA,KAAAe,OAAA,CAAAC,IAAA,UAAAhB,EAAA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}