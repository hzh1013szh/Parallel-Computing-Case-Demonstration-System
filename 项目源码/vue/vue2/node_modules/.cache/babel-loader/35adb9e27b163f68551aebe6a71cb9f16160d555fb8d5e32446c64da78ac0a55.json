{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: \"Layout\",\n  data() {\n    return {\n      user: localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")) : {}\n    };\n  },\n  methods: {\n    logout() {\n      localStorage.removeItem(\"user\");\n      this.$router.push(\"/login\");\n    }\n  }\n};","map":{"version":3,"names":["name","data","user","localStorage","getItem","JSON","parse","methods","logout","removeItem","$router","push"],"sources":["src/views/LayoutView.vue"],"sourcesContent":["\r\n<template>\r\n  <div>\r\n      <el-container>\r\n        <el-header style=\"background-color: #45494b\">\r\n          <img src=\"@/assets/logo.png\" alt=\"\" style=\"width: 40px; position: relative; top: 10px\">\r\n          <span style=\"font-size: 20px; color: whitesmoke ;margin-left: 15px\">并行计算案例展示平台</span>\r\n          <el-dropdown style=\"float: right;height: 60px;line-height: 60px\" trigger=\"click\">\r\n            <div class=\"circle\">\r\n              <img class=\"me-view-picture_avatar\" :src=\"'http://localhost:8080/api/files/' + user.avatar\"></img>\r\n            </div>\r\n            <el-dropdown-menu slot=\"dropdown\">\r\n              <el-dropdown-item @click.native=\"logout()\">退出登录</el-dropdown-item>\r\n            </el-dropdown-menu>\r\n          </el-dropdown>\r\n        </el-header>\r\n      </el-container>\r\n      <el-container>\r\n        <!--min-height是为了让他拉伸到底部        -->\r\n        <!--route.path表示我们现在选择的激活方式时路由模式 router表示激活路由模式-->\r\n        <el-aside style=\"overflow: hidden;min-height: 100vh; background-color: #545c64; width: 250px\">\r\n          <el-menu\r\n              :default-active=\"$route.path\"\r\n              router\r\n              class=\"el-menu-vertical-demo\"\r\n              background-color=\"#545c64\"\r\n              text-color=\"#fff\"\r\n              active-text-color=\"#ffd04b\">\r\n            <el-menu-item index=\"1\" :route=\"{name: 'home'}\">\r\n              <i class=\"el-icon-s-home\"></i>\r\n              <span slot=\"title\">首页</span>\r\n            </el-menu-item>\r\n            <el-menu-item index=\"2\" :route=\"{name: 'user',params:{id: user.id}}\">\r\n              <i class=\"el-icon-user\"></i>\r\n              <span slot=\"title\">个人中心</span>\r\n            </el-menu-item>\r\n            <el-submenu index=\"3\">\r\n              <template slot=\"title\">\r\n                <i class=\"el-icon-location\"></i>\r\n                <span>用户管理</span>\r\n              </template>\r\n              <el-menu-item-group>\r\n                <el-menu-item index=\"/admin\" :route=\"{name: 'admin'}\">用户信息</el-menu-item>\r\n              </el-menu-item-group>\r\n              <el-menu-item index=\"/audit\" :route=\"{name: 'audit'}\" v-if=\"user.role === 'admin'\">审核用户</el-menu-item>\r\n            </el-submenu>\r\n            <el-submenu index=\"4\" v-if=\"this.user.role === 'admin'\">\r\n              <template slot=\"title\">\r\n                <i class=\"el-icon-location\"></i>\r\n                <span>信息管理</span>\r\n              </template>\r\n              <el-menu-item-group>\r\n                <el-menu-item index=\"/notice\">系统公告</el-menu-item>\r\n              </el-menu-item-group>\r\n              <el-menu-item-group>\r\n                <el-menu-item index=\"/post/category\">需求分类</el-menu-item>\r\n              </el-menu-item-group>\r\n            </el-submenu>\r\n            <el-submenu index=\"5\">\r\n              <template slot=\"title\">\r\n                <i class=\"el-icon-location\"></i>\r\n                <span>需求管理</span>\r\n              </template>\r\n              <el-menu-item-group>\r\n                <el-menu-item index=\"/addpost\" :route=\"{name: 'addpost'}\">发帖</el-menu-item>\r\n              </el-menu-item-group>\r\n              <el-menu-item-group>\r\n                <el-menu-item index=\"/allpost\" :route=\"{name: 'allpost'}\">需求信息</el-menu-item>\r\n              </el-menu-item-group>\r\n            </el-submenu>\r\n          </el-menu>\r\n        </el-aside>\r\n        <el-main>\r\n\r\n          <router-view/>\r\n        </el-main>\r\n      </el-container>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Layout\",\r\n  data(){\r\n    return{\r\n      user: localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")) : {},\r\n    }\r\n  },\r\n  methods:{\r\n    logout(){\r\n      localStorage.removeItem(\"user\");\r\n      this.$router.push(\"/login\");\r\n    }\r\n  }\r\n}\r\n</script>\r\n<!--去除菜单栏右边多出来的一部分-->\r\n<style>\r\n.el-menu {\r\n  border-right: none !important;\r\n}\r\n.el-dropdown-link {\r\n  cursor: pointer;\r\n  color: #f4f5f6;\r\n}\r\n.el-icon-arrow-down {\r\n  font-size: 12px;\r\n}\r\n.circle{\r\n  margin-top: 5px;\r\n  height: 50px;\r\n}\r\n.me-view-picture_avatar{\r\n  object-fit: cover;\r\n  width: 50px;\r\n  height: 50px;\r\n  border: 1px solid #ddd;\r\n  border-radius: 50%;\r\n  vertical-align: top;\r\n  background-color: #ced5d0;\r\n}\r\n</style>"],"mappings":";AAiFA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,IAAA,EAAAC,YAAA,CAAAC,OAAA,WAAAC,IAAA,CAAAC,KAAA,CAAAH,YAAA,CAAAC,OAAA;IACA;EACA;EACAG,OAAA;IACAC,OAAA;MACAL,YAAA,CAAAM,UAAA;MACA,KAAAC,OAAA,CAAAC,IAAA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}