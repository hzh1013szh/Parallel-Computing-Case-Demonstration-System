n type=\"primary\" @click=\"edit(scope.row)\">编辑</el-button>\r\n            <el-popconfirm title=\"这是一段内容确定删除吗？\" @confirm=\"del(scope.row.id)\">\r\n              <el-button slot=\"reference\" type=\"danger\" style=\"margin-left: 5px\">删除</el-button>\r\n            </el-popconfirm>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n    </div>\r\n    <div style=\"margin-top: 15px\">\r\n      <el-pagination\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"params.pageNum\"\r\n          :page-sizes=\"[5, 10, 15, 20]\"\r\n          :page-size=\"params.pageSize\"\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n    <div>\r\n      <el-dialog title=\"请输入信息\" :visible.sync=\"dialogFormVisible\" width=\"30%\">\r\n        <el-form :model=\"form\">\r\n          <el-form-item label=\"公告标题\" label-width=\"15%\">\r\n            <el-input v-model=\"form.usenrid\" autocomplete=\"off\" style=\"width: 80%\"></el-input>\r\n          </el-form-item>\r\n        </el-form>\r\n        <el-form :model=\"form\">\r\n          <el-form-item label=\"公告内容\" label-width=\"15%\">\r\n            <el-input type=\"textarea\" v-model=\"form.name\" autocomplete=\"off\" style=\"width: 80%\"></el-input>\r\n          </el-form-item>\r\n        </el-form>\r\n        <div slot=\"footer\" class=\"dialog-footer\">\r\n          <el-button @click=\"cancel()\">取 消</el-button>\r\n          <el-button type=\"primary\" @click=\"submit()\">确 定</el-button>\r\n        </div>\r\n      </el-dialog>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport request from \"@/utils/request\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      // 查询参数\r\n      params:{\r\n        name: '',\r\n        pageNum: 1,\r\n        pageSize: 5,\r\n      },\r\n      tableData: [],\r\n      total : 0,\r\n      dialogFormVisible : false,\r\n      form:{\r\n        name: '',\r\n        content: '',\r\n        time: '',\r\n      },\r\n    }\r\n  },\r\n  // 生命周期 - 页面创建时会做的事情（访问当前this实例）\r\n  created() {\r\n    this.getBySearch();\r\n  },\r\n  // 定义页面触发一些事件调用的方法\r\n  methods: {\r\n    getBySearch() {\r\n      request.get(\"/notice/search\",{params: this.params}).then(res => {\r\n        if(res.code === \"0\"){\r\n          this.tableData = res.data.list;\r\n          this.total = res.data.total;\r\n        }\r\n        else{\r\n          this.$message({\r\n            type: 'error',\r\n            message: res.msg\r\n          });\r\n        }\r\n      })\r\n    },\r\n    add(){\r\n      this.form = {};\r\n      this.dialogFormVisible = true;\r\n    },\r\n    submit(){\r\n      // 管理员用户新增用户，初始化密码为1234\r\n      request.post(\"/notice\",this.form).then(res => {\r\n        if(res.code === \"0\") {\r\n          this.$message({\r\n            type: 'success',\r\n            message: '操作成功'\r\n          });\r\n          this.dialogFormVisible = false;\r\n          this.getBySearch();\r\n        }\r\n        else{\r\n          this.$message({\r\n            type: 'error',\r\n            message: res.msg\r\n          });\r\n        }\r\n      })\r\n    },\r\n    edit(currow){\r\n      this.dialogFormVisible = true;\r\n      this.form = currow;\r\n    },\r\n    del(id){\r\n      request.delete(`/notice/${id}`).then(res => {\r\n        if (res.code === \"0\") {\r\n          this.$message({\r\n            type: 'success',\r\n            message: '操作成功'\r\n          });\r\n          this.getBySearch();\r\n        } else {\r\n          this.$message({\r\n            type: 'error',\r\n            message: res.msg\r\n          });\r\n        }\r\n      })\r\n    },\r\n    cancel(){\r\n      this.dialogFormVisible = false;\r\n      this.getBySearch();\r\n    },\r\n    reset(){\r\n      this.params = {\r\n        name : '',\r\n        userid : '',\r\n        pageNum: 1,\r\n        pageSize: 5,\r\n      }\r\n